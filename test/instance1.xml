<?xml version="1.0" encoding="UTF-8"?>
<schema 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="urn:json-haze ../json-haze.xsd"
	xmlns="urn:json-haze">
	<object>
		<properties>
			<property name="fieldOne">
				<schema required="true">
					<number minimum="0" maximum="100"/>
				</schema>
			</property>
			<property name="fieldTwo">
				<schema>
					<string minLength="5" maxLength="10"/>
				</schema>
			</property>
			<property name="testReference">
					<schemaReference file="./testFragment.xml"/>
			</property>
		</properties>
		<additionalProperties>
			<false/>
		</additionalProperties>
	</object>
</schema>